[OVERRIDE]
verbosity=INFO

[DEFAULT]
#
# Number of probe threads
#
numThreads=5

#
# FCP Host
#
host=127.0.0.1

#
# FCP Port
#
port=9481

#
# Seconds to wait between probes per-thread.
#
probeWait=61

#
# Database location
#
databaseFile=database.sql

#
# DEBUG, INFO, WARNING
#
verbosity=WARNING

#
# Logfile location
#
logFile=probe.log

#
# Comma-delimited list of types. Every time a probe is sent its type is randomly
# selected from this list. Available types are documented in ProbeRequestMessage.java.
#
types=BANDWIDTH,BUILD,IDENTIFIER,LINK_LENGTHS,LOCATION,STORE_SIZE,UPTIME_48H,UPTIME_7D

#
# Hops to live
#
hopsToLive=20
